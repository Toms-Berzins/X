{
  "name": "powder-coating-pro",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.5",
    "@react-google-maps/api": "^2.20.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/classnames": "^2.3.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-helmet-async": "^1.0.1",
    "autoprefixer": "^10.4.0",
    "classnames": "^2.5.1",
    "cors": "^2.8.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "firebase": "^10.8.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.474.0",
    "nodemailer": "^6.9.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.5",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.1",
    "react-intersection-observer": "^9.15.1",
    "react-router-dom": "^6.29.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.5",
    "unsplash-js": "^7.0.19",
    "zod": "^3.24.1"
  },
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "cross-env PORT=3002 nodemon --exec node --loader ts-node/esm server/index.ts",
    "dev": "concurrently -n \"frontend,backend\" -c \"blue,green\" \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "build:server": "tsc -p tsconfig.server.json",
    "start:prod": "node dist/server/production.js",
    "build:prod": "npm run build && npm run build:server",
    "prod": "NODE_ENV=production npm run build:prod && npm run start:prod",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3001 cypress:run",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "create-admin": "node --loader ts-node/esm src/scripts/createAdminUser.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "A modern, responsive website for a powder coating business built with React, TypeScript, and Tailwind CSS.",
  "main": "tailwind.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.6",
    "@types/bonjour": "^3.5.13",
    "@types/estree": "^1.0.6",
    "@types/firebase": "^2.4.32",
    "@types/graceful-fs": "^4.1.9",
    "@types/http-errors": "^2.0.4",
    "@types/http-proxy": "^1.17.15",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^0.0.30",
    "@types/mime": "^3.0.4",
    "@types/node": "^22.13.0",
    "@types/q": "^1.5.8",
    "@types/qs": "^6.9.18",
    "@types/range-parser": "^1.2.7",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/serve-index": "^1.9.4",
    "@types/serve-static": "^1.15.7",
    "@types/sockjs": "^0.3.36",
    "@types/ws": "^8.5.14",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "axe-core": "^4.10.2",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.0.1",
    "cypress-axe": "^1.6.0",
    "http-proxy-middleware": "^2.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.4.2",
    "react-scripts": "^5.0.1",
    "start-server-and-test": "^2.0.10",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "vite": "^6.0.11"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
